<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>Bystd.DbFactory.pfx</AssemblyOriginatorKeyFile>
    <Version>22.8.16</Version>
    <PackageProjectUrl>https://gitee.com/bouyei/Bouyei.DbFactory</PackageProjectUrl>
    <RepositoryUrl>https://gitee.com/bouyei/Bouyei.DbFactory</RepositoryUrl>
    <Authors>bouyei</Authors>
    <PackageTags>database library</PackageTags>
    <PackageReleaseNotes>base on .net standard 2.0+ a database common lib</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>postgresql,oracle,sqlserver,sqlite,db2 etc.</Description>
    <Copyright>bouyei</Copyright>
    <AssemblyVersion>22.8.16</AssemblyVersion>
    <FileVersion>22.8.16</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="IBM.Data.DB2.Core" Version="3.1.0.600" />
    <PackageReference Include="MySql.Data" Version="8.0.30" />
    <PackageReference Include="Npgsql" Version="6.0.6" />
    <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="3.21.70" />
    <PackageReference Include="System.Data.Odbc" Version="6.0.0" />
    <PackageReference Include="System.Data.OleDb" Version="6.0.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.3" />
    <PackageReference Include="System.Data.SQLite.Core" Version="1.0.116" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="IBM.Data.DB2">
      <HintPath>..\..\DotNet\Bouyei.DbFactory\libs\IBM.Data.DB2.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
